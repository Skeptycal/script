_calendar_completion()
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    lists=`icalBuddy --help | awk '{print $1}' | grep "^['-]" | sed "s/'//g"`
    
    COMPREPLY=( $(compgen -W "${lists}" -- ${cur}) )
}
complete -F _calendar_completion calendar
