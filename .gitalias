[alias]
    alias = "!git config -l | grep alias | cut -c 7-"
    big-files = large-files
    br = branch
    bra = branch --all
	ci = commit -a
    cim = commit -am
    co = checkout
    d = diff --color-words
    di = diff --color-words
    diff = diff --color-words
    dt = difftool
	f = fetch
	fa = fetch --all
    find = !git-find-blob
	filelog = log -u
	fl = log -u
    forest = !git-forest
    history = whatchanged -p --follow
    history-rm = !git-history-rm
    i = info
    in = incoming
    incoming = !git-incoming
    info = !git-info
    large-files = !git-large-files
    ls = log --pretty=format:"%C(bold)%C(yellow)%<(8)%h%Creset\\ %C(yellow)%<(14)%ar%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --color
    ll = log --pretty=format:"%C(bold)%C(yellow)%<(8)%h%Creset\\ %C(yellow)%<(14)%C(yellow)%ar\\ %Cblue[%cn]\\ %Cred%d%Creset%n%s" --decorate --color --numstat
    m = merge
    mt = mergetool
    open = browse
    ours = checkout --ours 
    out = outgoing
    outgoing = !git-outgoing
    pci = !git ci && git push
    pu = push
    remotes = remote -v
    rm-all = !git-remove-all
    set-commit-date = !git-set-commit-date
    set-parents = !git-set-parents
    spu = !git svn fetch && git svn dcommit #!git-ext-svn-fetch && git-ext-svn-dcommit
    st = !git status && git --no-pager diff --stat --submodule
    sta = !git stash
    staged = !git --no-pager diff --staged
    sup = !git svn fetch && git svn rebase #!git-ext-svn-rebase && git-ext-svn-fetch
    super-gc = !git-super-ghc
    svnbr = !git-ext-branch
    svnci = !git-ext-commit -a
    svnco = !git-ext-checkout
    svnsta = !git-ext-stash
    theirs = checkout --theirs 
    up = pull
    wtf = !git-wtf
